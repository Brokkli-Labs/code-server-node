{
    "AWSEBDockerrunVersion": "1",
    "Image": {
      "Name": "brokkli/code-server-node",
      "Update": "true"
    },
    "Ports": [
      {
        "ContainerPort": "8080"
      }
    ],
    "Volumes": [
      {
        "HostDirectory": "/var/app/.config/code-server",
        "ContainerDirectory": "/home/coder/.config/code-server"
      },
      {
        "HostDirectory": "/var/app/.ssh",
        "ContainerDirectory": "/home/coder/.ssh"
      }
    ]
  }